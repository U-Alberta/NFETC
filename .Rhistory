require(quantmod)
getSymbols('APPL')
getSymbols('APPL')
setwd('../NLP/NFETC/')
library(Rtsne)
par(mfrow=c(2,1))
par(cex=0.7, mai=c(0.3,0.3,0.1,0.1))
train <- read.csv("plot/clean.csv")
Labels <- train$label
train$label <- as.factor(train$label)
colors = rainbow(length(unique(train$label)))
names(colors) = unique(train$label)
tsne <- Rtsne(train[,-1], dims=2, perplexity=10, verbose=TRUE, max_iter=800)
plot(tsne$Y, t="n", xlab="", ylab="")
text(tsne$Y, labels=train$label, col=colors[train$label])
idx <- c(4, 8, 18, 21, 29, 52, 62)
rect(tsne$Y[idx, 1]-2, tsne$Y[idx, 2]-1, tsne$Y[idx, 1]+2, tsne$Y[idx, 2]+1)
train <- read.csv("plot/full_hier.csv")
Labels <- train$label
train$label <- as.factor(train$label)
colors = rainbow(length(unique(train$label)))
names(colors) = unique(train$label)
tsne <- Rtsne(train[,-1], dims=2, perplexity=10, verbose=TRUE, max_iter=800)
plot(tsne$Y, t="n", xlab="", ylab="")
text(tsne$Y, labels=train$label, col=colors[train$label])
idx <- c(4, 8, 18, 21, 29, 52, 62)
rect(tsne$Y[idx, 1]-2, tsne$Y[idx, 2]-0.7, tsne$Y[idx, 1]+2, tsne$Y[idx, 2]+0.7)
